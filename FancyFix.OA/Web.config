<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="UploadConfig" type="FancyFix.Tools.Config.UploadConfig,Tools" />
  </configSections>
  <connectionStrings>
    <add name="ConnectionString" connectionString="Data Source=.;User ID=sa;Password=123;Initial Catalog=OA;" />
    <!--<add name="ConnectionString" connectionString="Data Source=.;User ID=yuzuoxiang;Password=yu759974394YU;Initial Catalog=OA;" />-->
    <!--<add name="ConnectionString" connectionString="Data Source=116.62.218.150;User ID=sa;Password=sameway2018;Initial Catalog=OA;" />-->
  </connectionStrings>
  <appSettings>
    <!--系统配置-->
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!--文本日志路径-->
    <add key="LogPath" value="/log" />
    <add key="filePath" value="E:\workspace\Fancy-fix\源代码\OA\FancyFix.OA\FancyFix.OA\Content\files" />
    <!--域名-->
    <add key="domain" value="fancyfix.com" />
    <add key="weburl" value="http://oa.fancyfix.com" />
    <add key="imgurl" value="http://img.fancyfix.com" />
    <!--超级管理员用户Id-->
    <add key="SuperAdminIds" value="1" />
    <!--价值观、KPI考核打分截止日-->
    <add key="WorkerEndDay" value="10" />
    <!--当月KPI进程生成截止日，上月KPI自评、价值观自评截止日-->
    <add key="CreateEndDay" value="9" />
    <!--系统全局 年份下拉框起始年份-->
    <add key="StartYear" value="2018" />
    <!--设计部门ID-->
    <add key="DesignDepartId" value="10" />
  </appSettings>
  <!--网站文件保存目录-->
  <UploadConfig>
    <SiteOptions>
      <add domain="local" url="http://oa.fancyfix.com/Content/files" folder="E:\workspace\Fancy-fix\源代码\OA\FancyFix.OA\FancyFix.OA\Content\files" />
      <add domain="files" url="http://img.fancyfix.com/files" folder="E:\workspace\Fancy-fix\源代码\OA\FancyFix.OA\FancyFix.OA\Content\files" />
    </SiteOptions>
    <Settings>
      <!--网站文件-->
      <add name="file" filePath="\web\file\" urlFilePath="/web/file/" allowUpload=".rar|.zip|.doc|.docx|.xls|.xlsx|.pdf|.pdfx|.txt" maxFileSize="52428800" createSmallPic="false" width="0" height="0" createMiddlePic="false" createBigPic="false" maxWidth="0" maxHeight="0" addWaterMark="false" waterMarkType="image" waterMarkImgOrTxt="/common/images/logo.png" transparency="1" />
      <!--网站视频-->
      <add name="video" filePath="\web\video\" urlFilePath="/web/video/" allowUpload=".mp4|.rmvb|.avi|.flv|.mkv|.rm|.mov|.wmv|.3gp|.m4v|.mpg|.mpeg|.rar|.zip" maxFileSize="314572800" createSmallPic="false" width="0" height="0" createMiddlePic="false" createBigPic="false" maxWidth="0" maxHeight="0" addWaterMark="false" waterMarkType="image" waterMarkImgOrTxt="/common/images/logo.png" transparency="1" />
      <!--网站图片-->
      <add name="web" filePath="\web\common\" urlFilePath="/web/common/" allowUpload=".gif|.jpg|.jpeg|.bmp|.png" maxFileSize="10484560" createSmallPic="true" width="250" height="250" createMiddlePic="false" createBigPic="false" maxWidth="2048" maxHeight="2048" addWaterMark="false" waterMarkType="image" waterMarkImgOrTxt="/common/images/logo.png" transparency="1" />
      <!--产品图片-->
      <add name="product" filePath="\product\pro\" urlFilePath="/product/pro/" allowUpload=".gif|.jpg|.jpeg|.bmp|.png" maxFileSize="10485760" createSmallPic="true" width="250" height="250" maxWidth="2048" maxHeight="2048" createMinPic="true" minWidth="70" minHeight="70" createMiddlePic="true" middleWidth="350" middleHeight="350" createBigPic="true" bigWidth="800" bigHeight="800" addWaterMark="false" waterMarkType="image" waterMarkImgOrTxt="/common/images/logo.png" transparency="1" />
      <!--产品详细内容图-->
      <add name="prodetail" filePath="\product\detail\" urlFilePath="/product/detail/" allowUpload=".gif|.jpg|.jpeg|.bmp|.png" maxFileSize="10485760" createSmallPic="false" width="250" height="250" maxWidth="2048" maxHeight="2048" createMiddlePic="false" createBigPic="false" addWaterMark="false" waterMarkType="image" waterMarkImgOrTxt="/common/images/logo.png" transparency="1" />
      <!--产品分类图-->
      <add name="proclass" filePath="\product\class\" urlFilePath="/product/class/" allowUpload=".gif|.jpg|.jpeg|.bmp|.png" maxFileSize="10485760" createSmallPic="true" width="156" height="156" maxWidth="2048" maxHeight="2048" createMiddlePic="false" createBigPic="false" addWaterMark="false" waterMarkType="image" waterMarkImgOrTxt="/common/images/logo.png" transparency="1" />
    </Settings>
  </UploadConfig>
  <system.web>
    <!--session状态保持-->
    <sessionState mode="StateServer" timeout="120" stateConnectionString="tcpip=127.0.0.1:42424" cookieless="false" />
    <httpCookies domain="fancyfix.com" />
    <compilation debug="false" targetFramework="4.6.2" />
    <httpRuntime executionTimeout="240" maxQueryStringLength="2097151 " maxRequestLength="2097151 " requestValidationMode="2.0" targetFramework="4.6.2" />
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules>
      <remove name="ApplicationInsightsWebTracking" />
    </modules>
    <security>
      <requestFiltering>
        <requestLimits maxQueryString="2147483647" maxAllowedContentLength="2147483647" />
      </requestFiltering>
    </security>
    <staticContent>
      <remove fileExtension=".eot" />
      <remove fileExtension=".svg" />
      <remove fileExtension=".woff" />
      <remove fileExtension=".woff2" />
      <mimeMap fileExtension=".woff" mimeType="application/font-woff" />
      <mimeMap fileExtension=".eot" mimeType="application/vnd.ms-fontobject" />
      <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
      <mimeMap fileExtension=".woff2" mimeType="application/x-font-woff" />
    </staticContent>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
        <directoryBrowse enabled="false" />
  </system.webServer>
  <runtime>
