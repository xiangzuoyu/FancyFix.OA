  <system.web>
    <!--session状态保持-->
    <sessionState mode="StateServer" timeout="120" stateConnectionString="tcpip=127.0.0.1:42424" cookieless="false" />
    <httpCookies domain="fancyfix.com" />
    <compilation debug="false" targetFramework="4.6.2" />
    <httpRuntime executionTimeout="240" maxQueryStringLength="2097151 " maxRequestLength="2097151 " requestValidationMode="2.0" targetFramework="4.6.1" />
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules>
      <remove name="ApplicationInsightsWebTracking" />
    </modules>
    <security>
      <requestFiltering>
        <requestLimits maxQueryString="2147483647" maxAllowedContentLength="2147483647" />
      </requestFiltering>
    </security>
    <staticContent>
      <remove fileExtension=".eot" />
      <remove fileExtension=".svg" />
      <remove fileExtension=".woff" />
      <remove fileExtension=".woff2" />
      <mimeMap fileExtension=".woff" mimeType="application/font-woff" />
      <mimeMap fileExtension=".eot" mimeType="application/vnd.ms-fontobject" />
      <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
      <mimeMap fileExtension=".woff2" mimeType="application/x-font-woff" />
    </staticContent>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
